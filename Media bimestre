programa
{
	
	funcao inicio()
	{
		real nota1
		real nota2

		escreva("Qual foi sua nota? ")
		leia(nota1)
		escreva("Qual foi sua outra nota? ")
		leia(nota2)

		real media = (nota1 + nota2) / 2
		
		se(media == 100) { 
			escreva("Aprovado com Distinção")
		}

		senao se(media >= 70 e media < 99) {
			escreva("Aprovado")
		}
				

		se(media < 70) {
			escreva("Reprovado")
		}
		
	}
}
