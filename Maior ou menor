programa
{
	// Atividade 1) Faça um programa que peça dois números e imprima o maior deles.
	
	funcao inicio()
	{
		inteiro num1
		inteiro num2

		escreva("informe o primeiro número: ")
		leia(num1)
		escreva("informe o segundo número: ")
		leia(num2)

		se(num1 > num2) {
			escreva("o maior é: ", num1)
		}

		se(num1 < num2) {
			escreva("o maior é: ", num2)
		}
	}
}
